/***********************************************\
| Internal Field: Pressure                      |
\***********************************************/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}

dimensions      [1 -1 -2 0 0 0 0];
internalField   uniform 101325;

boundaryField
{
    "(inlet_x|inlet_-x|inlet_y|inlet_-y)"
    {
        type            zeroGradient;
    }
    
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }
    
    walls
    {
        type            zeroGradient;
    }
}

