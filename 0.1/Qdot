FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.1";
    object      Qdot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    inlet_x
    {
        type            calculated;
        value           uniform 0;
    }
    inlet_-y
    {
        type            calculated;
        value           uniform 0;
    }
    inlet_y
    {
        type            calculated;
        value           uniform 0;
    }
    inlet_-x
    {
        type            calculated;
        value           uniform 0;
    }
}


