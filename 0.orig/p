    {
        type            zeroGradient;
    }
    
    inlet_y
    {
        type            zeroGradient;
    }
    
    inlet_-y
    {
        type            zeroGradient;
    }
    
    outlet
    {
        type            fixedValue;
        value           uniform 101325;
    }
    
    walls
    {
        type            zeroGradient;
    }
}

